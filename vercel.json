{
    "version": 2,
    "builds": [
      { "src": "index.html", "use": "@vercel/static" },
      { "src": "styles.css", "use": "@vercel/static" },
      { "src": "script.js", "use": "@vercel/static" },
      { "src": "projects/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/", "dest": "/index.html" },
      { "src": "/projects/project1/expanding-cards", "dest": "/projects/project1/expanding-cards/index.html" },
      { "src": "/projects/project1", "dest": "/projects/project1/index.html" },
      { "src": "/projects/(.*)", "dest": "/projects/$1" }
    ]
  }
  